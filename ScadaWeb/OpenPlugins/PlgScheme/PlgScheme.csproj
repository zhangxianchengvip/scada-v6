<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <RootNamespace>Scada.Web.Plugins.PlgScheme</RootNamespace>
    <Authors>Mikhail Shiryaev</Authors>
    <Company>Rapid Software LLC</Company>
    <Product>Rapid SCADA</Product>
    <Copyright>Copyright Â© 2024</Copyright>
    <Version>6.0.2</Version>
    <AssemblyVersion>6.0.2.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="compilerconfig.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="lang\PlgScheme.en-GB.xml" />
    <None Remove="lang\PlgScheme.ru-RU.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="lang\PlgScheme.ru-RU.xml" />
    <Content Include="lang\PlgScheme.en-GB.xml" />
  </ItemGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <None Include="compilerconfig.json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\ScadaCommon\ScadaCommon.Log\ScadaCommon.Log.csproj" />
    <ProjectReference Include="..\..\..\ScadaCommon\ScadaCommon\ScadaCommon.csproj" />
    <ProjectReference Include="..\..\ScadaWeb\ScadaWebCommon\ScadaWebCommon.csproj" />
    <ProjectReference Include="..\PlgScheme.Common\PlgScheme.Common.csproj" />
  </ItemGroup>

</Project>
